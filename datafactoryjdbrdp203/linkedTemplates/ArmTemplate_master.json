{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryjdbrdp203"
		},
		"Appdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Appdb'"
		},
		"datalaketestjdbr_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalaketestjdbr'"
		},
		"dp203synapsejdbr_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dp203synapsejdbr'"
		},
		"vmcito_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'vmcito'"
		},
		"datalaketestjdbr_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalaketestjdbr.dfs.core.windows.net/"
		},
		"dp203synapsejdbr_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dp203synapsejdbr.sql.azuresynapse.net"
		},
		"dp203synapsejdbr_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datapool"
		},
		"dp203synapsejdbr_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vespidboss"
		},
		"vmcito_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\inetpub\\logs\\LogFiles\\W3SVC1"
		},
		"vmcito_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "vespiboss"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryjdbrdp203_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Appdb_connectionString": {
						"value": "[parameters('Appdb_connectionString')]"
					},
					"datalaketestjdbr_accountKey": {
						"value": "[parameters('datalaketestjdbr_accountKey')]"
					},
					"dp203synapsejdbr_password": {
						"value": "[parameters('dp203synapsejdbr_password')]"
					},
					"datalaketestjdbr_properties_typeProperties_url": {
						"value": "[parameters('datalaketestjdbr_properties_typeProperties_url')]"
					},
					"dp203synapsejdbr_properties_typeProperties_server": {
						"value": "[parameters('dp203synapsejdbr_properties_typeProperties_server')]"
					},
					"dp203synapsejdbr_properties_typeProperties_database": {
						"value": "[parameters('dp203synapsejdbr_properties_typeProperties_database')]"
					},
					"dp203synapsejdbr_properties_typeProperties_userName": {
						"value": "[parameters('dp203synapsejdbr_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactoryjdbrdp203_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"vmcito_password": {
						"value": "[parameters('vmcito_password')]"
					},
					"vmcito_properties_typeProperties_host": {
						"value": "[parameters('vmcito_properties_typeProperties_host')]"
					},
					"vmcito_properties_typeProperties_userId": {
						"value": "[parameters('vmcito_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactoryjdbrdp203_ArmTemplate_0')]"
			]
		},
		{
			"name": "datafactoryjdbrdp203_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactoryjdbrdp203_ArmTemplate_1')]"
			]
		}
	]
}